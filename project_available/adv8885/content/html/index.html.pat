    $('.prallax').each(function () {
      var $bgobj = $(this);
      $(window).scroll(function () {
        var yPos = -(winPosTop / $bgobj.data('speed'));
        if ($bgobj.data('orientation') != 'h') {
          var coords = '100% ' + (50 + yPos) + '%';
        } else {
          var coords = (50 + yPos) + '% 50%';
        }
        $bgobj.css({backgroundPosition: coords});
      });
    });

